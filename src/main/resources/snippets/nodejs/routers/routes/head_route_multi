router.head('/{{resource_name}}/', function(req, res) {
	var query = {};
{{query_building}}
{{identifier_check}}
	{{model_name}}.find(query, function(err, docs) {
		if (err) {
			res.status(400).send();
		} else {
			if (Object.keys(query).length > 0 && docs.length === 0) {
				res.status(404).send();
			} else {
				res.status(200).send();
			}
		}
	});
});
