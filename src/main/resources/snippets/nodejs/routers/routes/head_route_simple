router.head('/{{resource_name}}/:identifier', function(req, res) {
	var query = {
		_id: req.params.identifier
	};
	{{query_by_type}}
	{{model_name}}.find(query, function(err, doc) {
		if (err){
			res.status(400).send();
		}
		else{
			if(doc)
				res.status(200).send();
			else
				res.status(404).send();
		}
	});
});
