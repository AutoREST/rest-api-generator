
router.patch('/{{resource_name}}/:identifier', function(req, res) {
	var query = {
		_id: req.params.identifier
	};
	{{query_by_type}}
	{{model_name}}.find(query, function(err, data) {
		if (err) {
			res.status(400).send(err);
		} else {
			if (data) {
				{{all_fields}}
				data.save(function(err) {
					if (err){
						res.status(400).send();
					}
					else{
						res.status(200).send(data.cleanObject());
					}
				});
			}
			else{
				res.status(404).send();
			}
		}
	});
});
